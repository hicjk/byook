@model Consumer

<div class="text-center mt-100">
    <h3 class=" mb-3">로그인</h3>
    <form method="post" asp-controller="Member" asp-action="Login">
        <div id="joinInfo" class="mt-5">
            <div>
                <input type="text" asp-for="Id" placeholder="아이디" />
            </div>
            <div>
                <input type="password" asp-for="Password" placeholder="비밀번호" />
            </div>
        </div>
        <div class="mb-3">
            <input class="btnRegister" type="submit" value="로그인" />
        </div>
    </form>
    <div>
        <a class="btnRegister" asp-controller="Member" asp-action="SellerLogin">
            <span>사업자 로그인</span>
        </a>
    </div>
</div>